{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/$1",
        "methods": ["GET"]
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/frontend/$1"
      }
    ]
  }
  